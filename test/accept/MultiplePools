start graph "multiplepools" 
    start pool "pool1"
        create event initial "startEvent" as $first
        create event intermediate "middle" as $midde
        connect $first to $middle
    end pool
    start pool "pool2"
        create event initial "startEvent" as $first2
        create event intermediate "middle" as $midde2
        connect $first to $middle2
    end pool
end graph