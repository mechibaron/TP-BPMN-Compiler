start graph "poolWithLane" 
    start pool "pool1"
        start lane "lane1"
            create event initial "startEvent" as $first
            create event intermediate "middle" as $middle
            connect $first to $middle
            start lane "lane2"
                create event initial "finalEvent" as $final
                connect $middle to $final
            end lane
        end lane
    end pool
end graph